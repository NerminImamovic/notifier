  "compilerOptions": {
    "outDir": "build",
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": false
  },
  "lib": ["es2015"]
}